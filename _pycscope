#compdef pycscope

_arguments -s  '-D[Dump (C)oncrete (S)yntax (T)ree generated byparser for each file]'\
  '-R[Recurse directories for files]'\
  '-S[Interpret simple strings as symbols]'\
  '(1 : 0)-V[Print version and exit]'\
  '-f[Use reffile as cross-ref file name instead of cscope.out]:reffile:_files:'\
  '-i[Usecontents of srclist file as list of source files to scan]'\
  '*:filename:_files'
