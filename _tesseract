           'poly_allow_detailed_fx=:Allow feature extractors to see the original'
           'poly_debug=:Debug old'
           'poly_wide_objects_better=:More accurate approx on wide'
           'preserve_interword_spaces=:Preserve multiple interword'
           'prioritize_division=:Prioritize blob division over'
           'quality_blob_pc=:good_quality_doc gte good blobs'
           'quality_char_pc=:good_quality_doc gte good char limit'
           'quality_min_initial_alphas_reqd=:alphas in a good'
           'quality_outline_pc=:good_quality_doc lte outline error'
           'quality_rej_pc=:good_quality_doc lte rejection limit'
           'quality_rowrej_pc=:good_quality_doc gte good char limit'
