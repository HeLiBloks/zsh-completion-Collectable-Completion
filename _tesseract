           'unlv_tilde_crunching=:Mark v.bad words for tilde'
           'unrecognised_char=:Output char for unidentified blobs'
           'use_ambigs_for_adaption=:Use ambigs for deciding whether to adapt to a'
           'use_definite_ambigs_for_classifier=:Use definite ambiguities when running character'
           'use_new_state_cost=:use new state cost heuristics for segmentation state'
           'use_only_first_uft8_step=:Use only the first UTF8 step of the given string when computing log probabilities'
           'user_patterns_file=:A filename of userprovided patterns.user_patterns_file:wp:_files'
           'user_patterns_suffix=:A suffix of userprovided patterns located in tessdata.user_patterns_suffix'
           'user_words_file=:A filename of userprovided words'
           'user_words_suffix=:A suffix of userprovided words located in tessdata.user_words_suffix'
           'word_to_debug=:Word for which stopper debug information should be printed to stdoutword_to_debug'
           'word_to_debug_lengths=:Lengths of unichars in word_to_debugword_to_debug_lengths'
           'wordrec_blob_pause=:Blob'
           'wordrec_debug_blamer=:Print blamer debug'
           'wordrec_debug_level=:Debug level for'
           'wordrec_display_all_blobs=:Display'
           'wordrec_display_all_words=:Display'
           'wordrec_display_segmentations=:Display'
           'wordrec_display_splits=:Display'
           'wordrec_enable_assoc=:Associator'
           'wordrec_max_join_chunks=:Max number of broken pieces to'
           'wordrec_no_block=:Dont output block'
           'wordrec_run_blamer=:Try to set the blame for'
           'wordrec_skip_no_truth_words=:Only run OCR for words that had truth recorded in'
           'wordrec_worst_state=:Worst segmentation'
           'words_default_fixed_limit=:Allowed size variance'
           'words_default_fixed_space=:Fraction of xheight'
           'words_default_prop_nonspace=:Fraction of xheight'
           'words_initial_lower=:Max initial cluster size'
           'words_initial_upper=:Min initial cluster spacing'
           'x_ht_acceptance_tolerance=:Max allowed deviation of blob top outside of font'
           'x_ht_min_change=:Min change in xht before actually trying'
           'xheight_penalty_inconsistent=:Score penalty 0.1  10% added if an xheight is inconsistent.'
           'xheight_penalty_subscripts=:Score penalty 0.1  10% added if there are subscripts or superscripts in a word, but it is otherwise OK.'
    )

    _describe 'parameters' parame && return 0
    ;;

esac
}

_tesseract "$@"
