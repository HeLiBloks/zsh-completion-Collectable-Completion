_tesseract() {

local -a arguments
local noargs context state line
local curcontext="$curcontext"

# contains commandline options already present in an associative array
typeset -A opt_args

# check and set commandline opts already present
(( $+opt_args[--tessdata-dir] )) && local TESSDATA_PREFIX=$opt_args[--tessdata-dir]

if [[ ! -d $TESSDATA_PREFIX/**/(/F)lang ]] ; then
  local TESSDATA_PREFIX
  if [[  -d "/usr/share/tesseract-ocr/[0-9].[0-9][0-9]" ]]; then
    TESSDATA_PREFIX="/usr/share/tesseract-ocr/[0-9].[0-9][0-9]"
  else
    [[ -d "/usr/share/tesseract-ocr" ]] || _message 'please set $TESSDATA_PREFIX!! for completion to work'
    TESSDATA_PREFIX="/usr/share/tesseract-ocr"
  fi
fi


noargs="( --print-parameters --list-langs -v --help-psm --help-oem --help -h --version )"

_arguments -C '1:input file:->input'\
  '2:output base:->output'\
  ':config files:->config'\
  $noargs'--tessdata-dir[Specify the location of tessdata path]:tessdata path:{_path_files -/ }'\
  $noargs'--user-words[Location of user words file]:user words:->userwords'\
  $noargs'--user-patterns[Location of user patterns file]:user patterns:->userpatterns'\
  $noargs'-l[Specify language used for OCR]:language:->language'\
  $noargs'-c[Set value for config variables]:config:->paramcomp'\
  $noargs'--psm[Specify page segmentation mode]:psm:((
    0\:Orientation\ and\ script\ detection\ OSD\ only\
    1\:Automatic\ page\ segmentation\ with\ OSD\
    2\:Automatic\ page\ segmentation,\ but\ no\ OSD,\ or\ OCR\
    3\:Fully\ automatic\ page\ segmentation,\ but\ no\ OSD\ Default\
    4\:Assume\ a\ single\ column\ of\ text\ of\ variable\ sizes\
    5\:Assume\ a\ single\ uniform\ block\ of\ vertically\ aligned\ text\
    6\:Assume\ a\ single\ uniform\ block\ of\ text\
    7\:Treat\ the\ image\ as\ a\ single\ text\ line\
    8\:Treat\ the\ image\ as\ a\ single\ word\
    9\:Treat\ the\ image\ as\ a\ single\ word\ in\ a\ circle\
    10\:Treat\ the\ image\ as\ a\ single\ character\
    11\:Sparse\ text.\ Find\ as\ much\ text\ as\ possible\ in\ no\ particular\ order\
    12\:Sparse\ text\ with\ OSD\
    13\:Raw\ line.\ Treat\ the\ image\ as\ a\ single\ text\ line,\ bypassing\ hacks\ that\ are\ Tesseract-specific\
  ))'\
  $noargs'--oem[Specify OCR Engine mode]:oem:((
    0\:Original\ Tesseract\ only\
    1\:Neural\ nets\ LSTM\ only\
    2\:Tesseract\ +\ LSTM\
    3\:Default\ based\ on\ what\ is\ available\
  ))'\
  '(- 1 :)'{-h,--help}'[Show this help message]'\
  '(- 1 :)--help-psm[Show page segmentation modes]'\
  '(- 1 :)--help-oem[Show OCR modes]'\
  '(- 1 :)'{-v,--version}'[Show version information]'\
  '(- 1 :)--list-langs[List available languages for tesseract engine]:tessdata path:(( --tessdata-dir\:tessdata\ dir ))'\
  '(- 1 :)--print-parameters[Print tesseract parameters to stdout]' && return 0

case "$state" in
    userwords)
        _alternative ":user words:_files $TESSDATA_PREFIX/**/???.user-words"\
            'files:current files:_files' && return 0
        ;;
    userpatterns)
        _alternative ":user patterns:_files $TESSDATA_PREFIX/**/???.user-patterns"\
            'files:current files:_files -g ???.user-patterns' && return 0
        ;;
    input)
        # _alternative ':input:infile:(( stdin\:"read data from stdin" ))'\
          # ':input:infile:_files' && return 0
        _files
        ;;
    output)
        # _alternative 'output:output base:(( stout\:"write data to stdout" ))'\
          # 'output:output base:_files' && return 0
        _files
        ;;
    config)
        local -a configfile
        local curcontext="$curcontext"
        configfile=(
            'hocr:Output in hOCR format'
            'pdf:If inovked with \[-psm 3\] outputs image and a separate searchable text layer with the recognized text underneath'
            'tsv:Output in tsv format'
            )
            _alternative 'outputformat:output format:{ _describe -V "conffiles" configfile }'\
                "files:config files:{_files -W $TESSDATA_PREFIX/tessdata/**configs }"
        ;;
    language)
        local curcontext="$curcontext"
        local -a lang
        lang=( $( tesseract --list-langs 2> /dev/null | sed '1d') )
        _values -s + 'flags' $lang
        ;;
    psmcomp)
        local curcontext="$curcontext"
        local -a psm
        psm=(
            '0:Orientation and script detection OSD only'
            '1:Automatic page segmentation with OSD'
            '2:Automatic page segmentation, but no OSD, or OCR'
            '3:Fully automatic page segmentation, but no OSD Default'
            '4:Assume a single column of text of variable sizes'
            '5:Assume a single uniform block of vertically aligned text'
            '6:Assume a single uniform block of text'
            '7:Treat the image as a single text line'
            '8:Treat the image as a single word'
            '9:Treat the image as a single word in a circle'
            '10:Treat the image as a single character'
            '11:Sparse text. Find as much text as possible in no particular order'
            '12:Sparse text with OSD'
            '13:Raw line. Treat the image as a single text line, bypassing hacks that are Tesseract-specific'
        )
        _describe -V 'page segmentation mode' psm
        ;;
    oemcomp)
        local curcontext="$curcontext"
        local -a oem
        oem=(
         '0:Original Tesseract only'
         '1:Neural nets LSTM only'
         '2:Tesseract + LSTM'
         '3:Default based on what is available'
          )
          _describe 'OCR mode' oem
        ;;
    paramcomp)
        #parame=$(tesseract --print-parameters | sed -re "1,3d; s/(\w+)		/\1	\1	/g ; s/([)(\"\'\`\´])//g; s/(\w+)\t(.+)\t(.+)/'\1=[\3 \\\[\2\\\]]:val:( \2 )'/g")
        local -a parame
        parame=(
           'allow_blob_division=:Use divisible blobs'
           'ambigs_debug_level=:Debug level for unichar'
           'applybox_debug=:Debug'
           'applybox_exposure_pattern=:Exposure value follows this pattern in the image filename. The name of the image files are expected to be in the form lang.fontname.exp num.tif.exp'
           'applybox_learn_chars_and_char_frags_mode=:Learn both character fragments as is done in the special low exposure mode as well as unfragmented characters'
           'applybox_learn_ngrams_mode=:Each bounding box is assumed to contain ngrams. Only learn the ngrams whose outlines overlap horizontally'
           'applybox_page=:Page number to apply boxes'
           'assume_fixed_pitch_char_segment=:include fixedpitch heuristics in char'
           'bestrate_pruning_factor=:Multiplying factor of current best rate to prune other'
           'bidi_debug=:Debug level for'
           'bland_unrej=:unrej potential with no'
           'certainty_scale=:Certainty scaling'
           'certainty_scale=:Certainty scaling'
           'chop_center_knob=:Split center adjustment'
           'chop_centered_maxwidth=:Width of smaller chopped blobs above which we dont care that a chop is not near the center.'
           'chop_debug=:Chop'
           'chop_enable=:Chop'
           'chop_good_split=:Good split'
           'chop_inside_angle=:Min Inside Angle'
           'chop_min_outline_area=:Min Outline'
           'chop_min_outline_points=:Min Number of Points on'
           'chop_new_seam_pile=:Use new'
           'chop_ok_split=:OK split'
           'chop_overlap_knob=:Split overlap adjustment'
           'chop_same_distance=:Same'
           'chop_seam_pile_size=:Max number of seams in'
           'chop_sharpness_knob=:Split sharpness adjustment'
           'chop_split_dist_knob=:Split length adjustment'
           'chop_split_length=:Split'
           'chop_vertical_creep=:Vertical'
           'chop_width_change_knob=:Width change'
           'chop_x_y_weight=:X / Y  length'
           'chs_leading_punct:Leading punctuation'
           'chs_trailing_punct1=:1st Trailing punctuation'
           'chs_trailing_punct2=:2nd Trailing punctuation'
           'classify_adapt_feature_threshold=:Threshold for good features during adaptive'
           'classify_adapt_proto_threshold=:Threshold for good protos during adaptive'
           'classify_adapted_pruning_factor=:Prune poor adapted results this much worse than best result'
           'classify_adapted_pruning_threshold=:Threshold at which classify_adapted_pruning_factor'
           'classify_bln_numeric_mode=:Assume the input is numbers '
           'classify_char_norm_range=:Character Normalization Range ...'
           'classify_character_fragments_garbage_certainty_threshold=:Exclude fragments that do not look like whole characters from training and'
           'classify_class_pruner_multiplier=:Class Pruner Multiplier 0255      '
           'classify_class_pruner_threshold=:Class Pruner Threshold'
           'classify_cp_angle_pad_loose=:Class Pruner Angle Pad'
           'classify_cp_angle_pad_medium=:Class Pruner Angle Pad'
           'classify_cp_angle_pad_tight=:CLass Pruner Angle Pad'
           'classify_cp_cutoff_strength=:Class Pruner CutoffStrength'
           'classify_cp_end_pad_loose=:Class Pruner End Pad Loose'
           'classify_cp_end_pad_medium=:Class Pruner End Pad Medium'
           'classify_cp_end_pad_tight=:Class Pruner End Pad Tight'
           'classify_cp_side_pad_loose=:Class Pruner Side Pad Loose'
           'classify_cp_side_pad_medium=:Class Pruner Side Pad Medium'
           'classify_cp_side_pad_tight=:Class Pruner Side Pad Tight'
           'classify_debug_character_fragments=:Bring up graphical debugging windows for fragments'
           'classify_debug_level=:Classify debug'
           'classify_enable_adaptive_debugger=:Enable match'
           'classify_enable_adaptive_matcher=:Enable adaptive'
           'classify_enable_learning=:Enable adaptive'
           'classify_font_name=:Default font name to be used in trainingUnknownFont:font'
           'classify_integer_matcher_multiplier=:Integer Matcher Multiplier  0255  '
           'classify_learn_debug_str=:Class str to debug learningclassify_learn_debug_str:str:'
           'classify_learning_debug_level=:Learning Debug Level:debug level:( 0 )'
           'classify_max_certainty_margin=:Veto difference between classifier certainties'
           'classify_max_norm_scale_x=:Max char xnorm scale ...'
           'classify_max_norm_scale_y=:Max char ynorm scale ...'
           'classify_max_rating_ratio=:Veto ratio between classifier ratings'
           'classify_max_slope=:Slope above which lines are called vertical'
           'classify_min_norm_scale_x=:Min char xnorm scale '
           'classify_min_norm_scale_y=:Min char ynorm scale '
           'classify_min_slope=:Slope below which lines are called horizontal'
           'classify_misfit_junk_penalty=:Penalty to apply when a nonalnum is vertically out of its expected textline'
           'classify_nonlinear_norm=:Nonlinear strokedensity'
           'classify_norm_adj_curl=:Norm adjust curl ...'
           'classify_norm_adj_midpoint=:Norm adjust midpoint ...'
           'classify_norm_method=:Normalization Method   ...'
           'classify_num_cp_levels=:Number of Class Pruner'
           'classify_pico_feature_length=:Pico Feature Length'
           'classify_pp_angle_pad=:Proto Pruner Angle'
           'classify_pp_end_pad=:Proto Prune End Pad'
           'classify_pp_side_pad=:Proto Pruner Side Pad'
           'classify_save_adapted_templates=:Save adapted templates to a'
           'classify_training_file=:Training fileMicroFeatures:file:_files'
           'classify_use_pre_adapted_templates=:Use preadapted classifier'
           'conflict_set_I_l_1=:Il1 conflict set Il'
           'crunch_accept_ok=:Use acceptability in'
           'crunch_debug=:As it'
           'crunch_del_cert=:POTENTIAL crunch cert lt'
           'crunch_del_high_word=:Del if word gt xht x this above bl'
           'crunch_del_low_word=:Del if word gt xht x this below bl'
           'crunch_del_max_ht=:Del if word ht gt xht x'
           'crunch_del_min_ht=:Del if word ht lt xht x this'
           'crunch_del_min_width=:Del if word width lt xht x'
           'crunch_del_rating=:POTENTIAL crunch rating lt'
           'crunch_early_convert_bad_unlv_chs=:Take out ~^ early'
           'crunch_early_merge_tess_fails=:Before word crunch'
           'crunch_include_numerals=:Fiddle alpha'
           'crunch_leave_accept_strings=:Dont pot crunch sensible'
           'crunch_leave_lc_strings=:Dont crunch words with long lower case'
           'crunch_leave_ok_strings=:Dont touch sensible'
           'crunch_leave_uc_strings=:Dont crunch words with long lower case'
           'crunch_long_repetitions=:Crunch words with long'
           'crunch_poor_garbage_cert=:crunch garbage cert lt'
           'crunch_poor_garbage_rate=:crunch garbage rating lt'
           'crunch_pot_garbage=:POTENTIAL crunch'
           'crunch_pot_indicators=:How many potential indicators'
           'crunch_pot_poor_cert=:POTENTIAL crunch cert lt'
           'crunch_pot_poor_rate=:POTENTIAL crunch rating lt'
           'crunch_rating_max=:For adj length in rating per'
           'crunch_small_outlines_size=:Small if lt xht x this'
           'crunch_terrible_garbage=:As it'
           'crunch_terrible_rating=:crunch rating lt'
           'cube_debug_level=:Print cube debug info'
           'dawg_debug_level=:Set to 1 for general debug info, to 2 for more details, to 3 to see all the debug'
           'debug_acceptable_wds=:Dump word pass/fail'
           'debug_file=:File to send tprintf output todebug_file:file:_files'
           'debug_fix_space_level=:Contextual fixspace'
           'debug_noise_removal=:Debug reassignment of small'
           'debug_x_ht_level=:Reestimate'
           'devanagari_split_debugimage=:Whether to create a debug image for split shirorekha process'
           'devanagari_split_debuglevel=:Debug level for split shirorekha process'
           'disable_character_fragments=:Do not include character fragments in the results of the'
           'doc_dict_certainty_threshold=:Worst certainty for words that can be inserted into thedocument dictionary'
           'doc_dict_pending_threshold=:Worst certainty for using pending'
           'docqual_excuse_outline_errs=:Allow outline errs in unrejection'
           'edges_boxarea=:Min area fraction of grandchild for box'
           'edges_childarea=:Min area fraction of child outline'
           'edges_children_count_limit=:Max holes allowed in'
           'edges_children_fix=:Remove boxy parents of charlike'
           'edges_children_per_grandchild=:Importance ratio for chucking'
           'edges_debug=:turn on debugging for this'
           'edges_max_children_layers=:Max layers of nested children inside a character'
           'edges_max_children_per_outline=:Max number of children inside a character'
           'edges_min_nonhole=:Min pixels for potential char in'
           'edges_patharea_ratio=:Max lensq/area for acceptable child'
           'edges_use_new_outline_complexity=:Use the new outline complexity'
           'editor_dbwin_height=:Editor debug window'
           'editor_dbwin_name=:Editor debug window nameEditorDBWin'
           'editor_dbwin_width=:Editor debug window'
           'editor_dbwin_xpos=:Editor debug window X'
           'editor_dbwin_ypos=:Editor debug window Y'
           'editor_debug_config_file=:Config file to apply to single wordseditor_debug_config_file:file:_files'
           'editor_image_blob_bb_color=:Blob bounding box'
           'editor_image_menuheight=:Add to image height for menu bar'
           'editor_image_text_color=:Correct text'
           'editor_image_win_name=:Editor image window nameEditorImage'
           'editor_image_word_bb_color=:Word bounding box'
           'editor_word_height=:Word window'
           'editor_word_name=:BL normalized word windowBlnWords:word'
           'editor_word_width=:Word window'
           'editor_word_xpos=:Word window X'
           'editor_word_ypos=:Word window Y'
           'enable_new_segsearch=:Enable new segmentation search path'
           'enable_noise_removal=:Remove and conditionally reassign small outlines when they confuse layout analysis, determining diacritics vs'
           'equationdetect_save_bi_image=:Save input bi'
           'equationdetect_save_merged_image=:Save the merged'
           'equationdetect_save_seed_image=:Save the seed'
           'equationdetect_save_spt_image=:Save special character'
           'file_type=:Filename extension.tif'
           'fixsp_done_mode=:What constitues done for'
           'fixsp_non_noise_limit=:How many nonnoise blbs either side'
           'fixsp_small_outlines_size=:Small if lt xht x this'
           'force_word_assoc=:force associator to run regardless of what enable_assoc is.This is used for CJK where component grouping is necessary'
           'fragments_debug=:Debug character'
           'fragments_guide_chopper=:Use information from fragments to guide chopping'
           'fx_debugfile=:Name of debugfileFXDebug:file:_files'
           'gapmap_big_gaps=:xht multiplier'
           'gapmap_debug=:Say which blocks have'
           'gapmap_no_isolated_quanta=:Ensure gaps not less than 2quanta'
           'gapmap_use_ends=:Use large space at start and end of'
           'heuristic_max_char_wh_ratio=:max char widthtoheight ratio allowed in'
           'heuristic_segcost_rating_base=:base factor for adding segmentation cost into word rating.Its a multiplying factor, the larger the value above 1, the bigger the effect of segmentation cost.'
           'heuristic_weight_rating=:weight associated with char rating in combined cost'
           'heuristic_weight_seamcut=:weight associated with seam cut in combined cost of'
           'heuristic_weight_width=:weight associated with width evidence in combined cost of'
           'hocr_font_info=:Add font info to hocr'
           'hyphen_debug_level=:Debug level for hyphenated words'
           'il1_adaption_test=:Dont adapt to i/I at beginning of'
           'include_page_breaks=:Include page separator string in output text after each image/page'
           'interactive_display_mode=:Run interactively'
           'language_model_debug_level=:Language model debug'
           'language_model_fixed_length_choices_depth=:Depth of blob choice lists to explore when fixed length dawgs are'
           'language_model_min_compound_length=:Minimum length of compound'
           'language_model_ngram_nonmatch_score=:Average classifier score of a nonmatching unichar.'
           'language_model_ngram_on=:Turn on/off the use of character ngram'
           'language_model_ngram_order=:Maximum order of the character ngram'
           'language_model_ngram_rating_factor=:Factor to bring logprobs into the same range as ratings when multiplied by outline length'
           'language_model_ngram_scale_factor=:Strength of the character ngram model relative to the character classifier'
           'language_model_ngram_small_prob=:To avoid overly small denominators use this as the floor of the probability returned by the ngram model.'
           'language_model_ngram_space_delimited_language=:Words are delimited by'
           'language_model_ngram_use_only_first_uft8_step=:Use only the first UTF8 step of the given string when computing log probabilities'
           'language_model_penalty_case=:Penalty for inconsistent case'
           'language_model_penalty_chartype=:Penalty for inconsistent character type'
           'language_model_penalty_font=:Penalty for inconsistent'
           'language_model_penalty_increment=:Penalty increment'
           'language_model_penalty_non_dict_word=:Penalty for nondictionary words'
           'language_model_penalty_non_freq_dict_word=:Penalty for words not in the frequent word dictionary'
           'language_model_penalty_punc=:Penalty for inconsistent punctuation'
           'language_model_penalty_script=:Penalty for inconsistent script'
           'language_model_penalty_spacing=:Penalty for inconsistent spacing'
           'language_model_use_sigmoidal_certainty=:Use sigmoidal score for'
